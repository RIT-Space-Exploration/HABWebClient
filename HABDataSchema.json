{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "HAB Data Buffer",
  "description": "Sensor readings from a High Altitude Balloon(HAB)",
  "type": "object",
  "properties": {
    "dateCreated": {
    "type": "number"
    },
    "name": {
      "type": "string"
    },
    "payload": {
      "type": "object",
      "properties": {
        "timestamp": {"type": "number"},
        "temp_1": {"type": "number"},
        "pressure": {"type": "number"},
        "altitude": {"type": "number"},
        "humidity": {"type": "number"},
        "imu_gx": {"type": "number"},
        "imu_gy": {"type": "number"},
        "imu_gz": {"type": "number"},
        "imu_ax": {"type": "number"},
        "imu_ay": {"type": "number"},
        "imu_az": {"type": "number"},
        "imu_mx": {"type": "number"},
        "imu_my": {"type": "number"},
        "imu_mz": {"type": "number"},
        "temp_2": {"type": "number"},
      }
    }
  }
}